  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "ng build",
    "build:api": "nx run api:build",
    "build:api:prod": "nx run api:build --configuration=production",
    "build:compodoc": "compodoc -p tsconfig.base.json -d dist/docs",
    "build:playground": "nx build --project playground --extra-webpack-config apps/playground/webpack.partial.js",
    "build:storybook": "build-storybook -s ./libs/shared-assets",
    "compodoc:json": "compodoc -p ./tsconfig.base.json -e json -d ./.storybook",
    "dep-graph": "nx dep-graph",
    "e2e": "ng e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && ng lint",
    "ng": "nx",
    "nx": "nx",
    "pm2:del:all": "pm2 delete all",
    "pm2:logs": "pm2 logs",
    "pm2:ls": "pm2 list",
    "pm2:monit": "pm2 monit",
    "pm2:reload:all": "pm2 reload all",
    "pm2:restart:all": "pm2 restart all",
    "pm2:serve:api": "pm2 start tools/pm2.api.js --wait-ready",
    "pm2:serve:compodoc": "pm2 start ./tools/pm2.serve-compodoc.js --wait-ready",
    "pm2:stop:all": "pm2 stop all",
    "pm2:update": "pm2 update",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:api": "nx run api:serve",
    "serve:compodoc:": "compodoc -p tsconfig.base.json -d dist/docs -s -p 4202",
    "serve:playground": "nx serve --project playground --extra-webpack-config apps/playground/webpack.partial.js",
    "serve:storybook": "yarn run compodoc:json && yarn run start-storybook -p 4444 -s ./libs/assets",
    "start": "ng serve",
    "test": "ng test",
    "transloco:extract-scoped-libs": "transloco-scoped-libs",
    "transloco:extract-scoped-libs:watch": "transloco-scoped-libs --watch",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic"
  },
